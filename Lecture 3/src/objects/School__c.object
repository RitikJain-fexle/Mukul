    <validationRules>
        <fullName>VR_Validate_Date_of_Establishment</fullName>
        <active>true</active>
        <description>Validateion rule to make sure that an user can&apos;t out a future year value in the Date of Establishment field for the school object</description>
        <errorConditionFormula>IF(
    OR(
        YEAR(Year_of_Est__c) &gt; YEAR(TODAY()),
        AND(
            YEAR(Year_of_Est__c) = YEAR(TODAY()),
            MONTH(Year_of_Est__c) &gt; MONTH(TODAY())
        ),
        AND(
            YEAR(Year_of_Est__c) = YEAR(TODAY()),
            MONTH(Year_of_Est__c) = MONTH(TODAY()),
            DAY(Year_of_Est__c) &gt; DAY(TODAY())
        )
    ),
    true,
    false
)</errorConditionFormula>
        <errorMessage>Please Enter a valid value for Date of Establishment</errorMessage>
    </validationRules>
